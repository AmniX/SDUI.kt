{
    "type": "column",
    "id": "components-demo",
    "children": [
        {
            "type": "text",
            "id": "title",
            "text": "UI Components Demo",
            "style": {"fontSize": 20, "fontWeight": "bold"}
        },
        {"type": "spacer", "id": "s1", "height": 16},
        {
            "type": "switch",
            "id": "notifications",
            "label": "Enable Notifications",
            "checked": true
        },
        {"type": "spacer", "id": "s2", "height": 12},
        {
            "type": "checkbox",
            "id": "terms",
            "label": "I agree to terms and conditions",
            "checked": false
        },
        {"type": "spacer", "id": "s3", "height": 12},
        {
            "type": "slider",
            "id": "volume",
            "label": "Volume",
            "value": 50.0,
            "minValue": 0.0,
            "maxValue": 100.0
        },
        {"type": "spacer", "id": "s4", "height": 12},
        {
            "type": "chip",
            "id": "tag",
            "text": "Selected Tag",
            "selected": true
        },
        {"type": "spacer", "id": "s5", "height": 16},
        {
            "type": "progressBar",
            "id": "progress",
            "progress": 0.7,
            "label": "Progress: 70%"
        }
    ]
}